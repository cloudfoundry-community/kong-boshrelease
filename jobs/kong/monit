check process avi-controller
  with pidfile /var/vcap/sys/run/avi-controller/avi-controller.pid
  start program "/var/vcap/jobs/avi-controller/bin/monit_debugger ctl '/var/vcap/jobs/avi-controller/bin/ctl start'"
  stop program "/var/vcap/jobs/avi-controller/bin/monit_debugger ctl '/var/vcap/jobs/avi-controller/bin/ctl stop'"
  group vcap
